<clickhouse>

    <!-- Profiles. -->
    <profiles>
        <default>
            <send_timeout>1500</send_timeout>
            <allow_experimental_database_replicated>1</allow_experimental_database_replicated>
            <distributed_ddl_output_mode>throw_only_active</distributed_ddl_output_mode>
        </default>
        <root>
            <send_timeout>3000</send_timeout>
            <allow_experimental_database_replicated>1</allow_experimental_database_replicated>
            <distributed_ddl_output_mode>throw_only_active</distributed_ddl_output_mode>
        </root>
        <test>
            <send_timeout>2000</send_timeout>
            <allow_experimental_database_replicated>1</allow_experimental_database_replicated>
            <distributed_ddl_output_mode>throw_only_active</distributed_ddl_output_mode>
        </test>
    </profiles>

    <!-- Users and ACL. -->
    <users>
        <default>
            <password></password>
            <access_management>0</access_management>
            <named_collection_control>0</named_collection_control>
            <show_named_collections_secrets>0</show_named_collections_secrets>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
        </default>
        <test>
            <password_sha256_hex>34633f8ff756b94d7102881abfb97641d05c77e4593d78bd2093012a278f38f0</password_sha256_hex>
            <access_management>0</access_management>
            <named_collection_control>0</named_collection_control>
            <show_named_collections_secrets>0</show_named_collections_secrets>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>test</profile>
            <quota>default</quota>
        </test>
        <!-- Superuser -->
        <root>
            <password>rootpass</password>
            <access_management>1</access_management>
            <named_collection_control>1</named_collection_control>
            <show_named_collections_secrets>1</show_named_collections_secrets>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>root</profile>
            <quota>default</quota>
        </root>
    </users>

    <!-- Quotas -->
    <quotas>
        <!-- No limits. Just calculate resource usage for time interval. -->
        <default>
            <!-- Limits for time interval. You could specify many intervals with different limits. -->
            <interval>
                <!-- Length of interval. -->
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>

</clickhouse>
